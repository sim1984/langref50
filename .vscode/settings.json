{
    "asciidoc.antora.showEnableAntoraPrompt": false,
    "[asciidoc]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 120
    },
    "editor.rulers": [120],
    "editor.tabSize": 4,
    "cSpell.language": "ru,en",
    "cSpell.maxNumberOfProblems": 10000,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "cSpell.words": [
        "ACOSH", "ASINH",
        "DATEADD", "DATEDIFF", "DBKEY", "DECFLOAT",
        "GDSCODE",
		"LOCALTIME", "LOCALTIMESTAMP", "LPAD",
        "MAXVALUE", "MINVALUE", "NCHAR", "NTILE", "NULLIF",
        "PSQL", "RPAD", "TOTALORDER", "TRUNC",
		"SAVEPOINT", "SMALLINT", "SQLCODE", "SQLSTATE", "STDDEV", "SYSDBA",
        "VARBINARY", "VARCHAR",
        "подзапрос", "подзапроса", "подзапросов", "подзапросом", "подзапросы", "подзапросами"
    ]
}
